body[data-sidebar="dark"] .mm-active .active i {
    /*color: #fff !important;*/
}
body[data-sidebar="dark"] .menu-title {
    color: #fff !important;
}
body[data-layout="horizontal"] .main-content {
    margin-left: 0 !important;
}
body[data-sidebar-size="small"] .navbar-brand-box {
    width: 160px;
}
